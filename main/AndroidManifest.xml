<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tcet.rcare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="R-Care"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tcet.rcare.NotepadEntry"></activity>
        <activity
            android:name="com.tcet.rcare.MainActivity"
            android:clearTaskOnLaunch="true"
            android:finishOnTaskLaunch="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tcet.rcare.intro.IntroActivity" />

        <receiver android:name="com.tcet.rcare.notification.AlarmReceiver" />
    </application>

</manifest>